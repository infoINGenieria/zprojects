	<queryString>
		<![CDATA[SELECT 
R.comida, R.vianda, R.vianda_desa,
IH.*
FROM partediario PD
INNER JOIN operarios O ON PD.operario = O.id
INNER JOIN informes_horas IH ON PD.operario = IH.id_operario 
INNER JOIN registro R ON PD.horario = R.id

where IH.id = $P{id_reporte} and  PD.fecha <= IH.hasta_f and PD.fecha >= IH.desde_f]]>
	</queryString>
